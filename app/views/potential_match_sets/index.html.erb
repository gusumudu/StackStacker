<h1>Listing potential_match_sets</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @potential_match_sets.each do |potential_match_set| %>
      <tr>
        <td><%= link_to 'Show', potential_match_set %></td>
        <td><%= link_to 'Edit', edit_potential_match_set_path(potential_match_set) %></td>
        <td><%= link_to 'Destroy', potential_match_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Potential match set', new_potential_match_set_path %>
